<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ProjectPanda.Views.ProfilePageViews.UserPrivateDetailedInfo.SearchMedicalCondition"
             Padding="15"
             BackgroundColor="{StaticResource BackgroundColor}"
             >
    <ContentPage.Content>
        <Frame BackgroundColor="White" CornerRadius="10" VerticalOptions="Start">
            <SearchBar CancelButtonColor="Red" SearchButtonPressed="SearchBar_SearchButtonPressed" Placeholder="Search" PlaceholderColor="Black" HeightRequest="50"/>
        </Frame>
    </ContentPage.Content>
</ContentPage>